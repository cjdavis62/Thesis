@article{1367-2630-6-1-139,
  author={Gianluigi Fogli and Eligio Lisi},
  title={Evidence for the MSW effect},
  journal={New Journal of Physics},
  volume={6},
  number={1},
  pages={139},
  url={http://stacks.iop.org/1367-2630/6/i=1/a=139},
  year={2004},
  abstract={Recent solar and reactor neutrino data have convincingly established that electron neutrinos and antineutrinos are subject to flavour transitions driven by neutrino masses and mixing. In addition, such data can be used to prove that the interaction of neutrinos in matter modifies the flavour transition pattern with respect to the case of propagation in vacuum, as predicted long ago by Mikheyev, Smirnov and Wolfenstein (MSW). We present a brief review of how the current evidence for MSW solar neutrino transitions has developed in recent years, and how it has been strengthened by the latest reactor neutrino data presented at the Neutrino 2004 Conference.}
}

